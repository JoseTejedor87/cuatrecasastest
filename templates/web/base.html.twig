{% trans_default_domain "messages" %}
<!DOCTYPE html>
<html lang="es">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link href="https://www.cuatrecasas.com/favicon.ico" rel="shortcut icon" />
        <link rel="canonical" href="{{ url(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) }}" />
        <title>{% block title %}{{ navigation_service.getPageTitle() }}{% endblock %}</title>
        {% include 'web/tools/hreflang.html.twig' %}
        {% include 'web/tools/stylesheets.html.twig' %}
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% include 'web/tools/menu.html.twig' %}
        {% block main %}{% endblock %}
        {% include 'web/tools/footer.html.twig' %}
        {% include 'web/tools/javascripts.html.twig' %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
