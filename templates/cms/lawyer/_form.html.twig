{% trans_default_domain "admin" %}

<div class="row">
    <div class="col-md-4">
        {{ form_row(form.name) }}
        {{ form_row(form.surname) }}
        {{ form_row(form.email) }}
        {{ form_row(form.phone) }}
        {{ form_row(form.fax) }}
        {{ form_row(form.photo) }}
        {{ form_row(form.lawyerType) }}
        {{ form_row(form.languages) }}
    </div>
    <div class="col-md-8">
        {{ form_row(form.translations) }}
    </div>
    {{ form_rest(form) }}
</div>
