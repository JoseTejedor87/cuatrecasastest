<!DOCTYPE html>

<title>Speaker</title>

{% block body %}
    <h1>Speaker</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>CreatedAt</th>
                <td>{{ speaker.createdAt ? speaker.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ speaker.updatedAt ? speaker.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Languages</th>
                <td>{{ speaker.languages ? speaker.languages|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Locations</th>
                <td>{{ speaker.locations ? speaker.locations|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ speaker.name }}</td>
            </tr>
            <tr>
                <th>Surname</th>
                <td>{{ speaker.surname }}</td>
            </tr>
            <tr>
                <th>Position</th>
                <td>{{ speaker.position }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ speaker.id }}</td>
            </tr>
            <tr>
                <th>OldId</th>
                <td>{{ speaker.oldId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('speaker_index') }}">back to list</a>

    <a href="{{ path('speaker_edit', {'id': speaker.id}) }}">edit</a>

    {{ include('speaker/_delete_form.html.twig') }}
{% endblock %}
